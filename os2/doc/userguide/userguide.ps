/tabbrest { % usage (n) x yn x y tabbrest - multibar rest
   gsave moveto currentpoint 0.8 setlinewidth
   0 -6 rmoveto 0 12 rlineto 40 0 rmoveto 0 -12 rlineto stroke
   3 setlinewidth moveto 40 0 rlineto stroke
   moveto 20 0 rmoveto /Times-Bold 13 selectfont cshow grestore
} bind def

/tabdt { % usage: x y n tabdt - n dots after flag
  3 1 roll moveto 1 1 rmoveto currentpoint 3 2 roll
  {
    exch 3 add exch newpath
    1 0 360 arc
    currentpoint fill closepath
  } repeat
  pop pop
} bind def

/tabfbourdon { % usage: x n (c) - n-th bourdon stopped at c in frenchtab
  gsave .75 setlinewidth
  3 1 roll 1 index
  -1.0 add -2 moveto
  { -7.0 -12 rlineto 4.5 12 rmoveto} repeat stroke
  -14 1 add moveto
  /FrenchTabfont 14.0 selectfont
  /bx false def cshow grestore
} bind def

/tabf1bourdon { % usage: x n (c) - n-th bourdon stopped at c in frenchtab
  gsave .75 setlinewidth
  exch 3 -1 roll 2 copy 
  1 index .5 mul neg -7.0 add add -7 moveto
  3 -1 roll
  {14 4 rlineto -13.5 -7 rmoveto} repeat stroke
  -3 mul -14 add 1 add moveto
  /FrenchTabfont 14.0 selectfont
  /bx false def cshow grestore
} bind def

/tabibourdon { % usage: x (c) tabibourdon - (c) on 7-th course in italiantab
  gsave .75 setlinewidth
  exch dup 84 moveto -7 0 rmoveto 15 0 rlineto stroke
  0 3 -1 roll tabsshow grestore
} bind def

/tabbar { % usage: x h tabbar - single bar in tab
  exch 0 moveto 0 exch rlineto stroke
} bind def

/tabdbar { % usage: x h tabdbar - thin double bar in tab
  exch 0 moveto dup 0 exch rlineto dup -3 exch neg rmoveto
  0 exch rlineto stroke
} bind def

/tabfbar1 { % usage: x h tabfbar1 - fat double bar at start
  exch 0 moveto dup 0 exch rlineto 3 0 rlineto 
  dup 0 exch neg rlineto currentpoint fill moveto
  3 0 rmoveto dup 0 exch rlineto stroke
} bind def

/tabfbar2 { % usage: x tabfbar2 - fat double bar at end
  exch 0 moveto dup 0 exch rlineto -3 0 rlineto 
  dup 0 exch neg rlineto currentpoint fill moveto
  -3 0 rmoveto dup 0 exch rlineto stroke
} bind def

/tabrdots { % usage: x n rdots - n repeat dots
  exch 0 moveto 0 7.0 rmoveto currentpoint 2 copy 1.2 0 360 arc
  3 -1 roll 2 sub {moveto 0 14 rmoveto  currentpoint 2 copy 1.2 0 360 arc} repeat
  moveto 0 14 rmoveto  currentpoint 1.2 0 360 arc fill
} bind def

/tabend1 { % usage: x1 x2 h (str) tabend1 - mark first ending
  4 1 roll dup 3 1 roll moveto 0 6 rlineto 2 copy 6 add lineto 0 -6 rlineto stroke
  moveto 4 -4 rmoveto gsave /Times-Roman 13 selectfont 1.2 0.95 scale
  show grestore
} bind def

/tabend2 { % usage: x1 x2 h (str) tabend2 - mark second ending
  4 1 roll dup 3 1 roll 6 add moveto 2 copy 6 add lineto 0 -6 rlineto stroke
  moveto 4 -4 rmoveto gsave /Times-Roman 13 selectfont 1.2 0.95 scale
  show grestore
} bind def

/tabhld { % usage: x y tabhld - fermata
  newpath /x 3 -1 roll def hld
} bind def

/tabtrl { % usage: x tabtrl - trill
  gsave /Times-BoldItalic 14 selectfont
  4 sub -17.5 moveto (tr.) show grestore
} bind def

/tabacc { % usage x n tabacc - accent from above
  -6 add -14 mul -2.5 add moveto
  1 1 2 1 2 4 rcurveto
  currentpoint 1.5 add 1.5 -90 0 arcn
  0 -3 -3 -3.5 -3.5 -5.5 rcurveto fill
} bind def

/tabx { % usage x n tabx - mordent
  -6 add -14 mul -3.5 add moveto
  gsave 0.7 setlinewidth
  5 7 rlineto -5 0 rmoveto 5 -7 rlineto
  stroke grestore
} bind def

/tabu { % usage x n tabu - arc starting at x
  -6 add -14 mul -2 add moveto
  gsave 1.0 setlinewidth
  2 -4 5 -4 7 0 rcurveto
  stroke grestore
} bind def

/tabv { % usage x n tabu - arc centered at x
  -6 add -14 mul -2 add exch -3.5 add exch moveto
  gsave 1.0 setlinewidth
  2 -4 5 -4 7 0 rcurveto
  stroke grestore
} bind def

/tabstar { % usage x n tabstar - asterisque
  -6 add -14 mul -7.0 add moveto
  /Helvetica 21 selectfont (*) show
} bind def

/tabcross { % usage x n tabcross - double cross
  -6 add -14 mul -1 add moveto
  /Helvetica 14 selectfont (#) show
} bind def

/taboline { % usage x n taboline - oblique line
  -5.5 add -14 mul moveto
  gsave 1.2 setlinewidth
  -5 -1.5 rmoveto 12 3 rlineto stroke grestore
} bind def

/tabi {  % usage: x n tabi - index finger
  -5.5 add -14 mul moveto
  currentpoint 2 copy 1.2 0 360 arc fill
} bind def

/tabm {  % usage: x n tabm - middle finger
  -5.5 add -14 mul moveto -1.3 0 rmoveto
  currentpoint 2 copy 1.2 0 360 arc
  currentpoint fill moveto 2.6 0 rmoveto
  currentpoint 2 copy 1.2 0 360 arc fill
