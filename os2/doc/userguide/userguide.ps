% enable new font
/BuildChar {
20 0
0 0 20 20
setcachedevice
/char exch def
0.1 setlinewidth
begin
  Encoding char get
  drawchar exch get
  exec
  end
}def

end

/ItalianTabfont itabfont definefont pop
%----------------------------------------------------------------

/cshow { % string cshow - center at current pt
   dup stringwidth pop 2 div neg 0 rmoveto
   bx {box} if show
} bind def

/lshow { % string lshow - show left-aligned
   dup stringwidth pop neg 0 rmoveto bx {box} if show
} bind def

/rshow { % string rshow - show right-aligned
   bx {box} if show
} bind def

/box { % str box - draw box around string
  gsave 0.5 setlinewidth dup stringwidth pop
  -2 -2 rmoveto 4 add fh 4 add 2 copy
  0 exch rlineto 0 rlineto neg 0 exch rlineto neg 0 rlineto
  stroke grestore
} bind def

/wd { moveto bx {box} if show } bind def
/wln {
dup 3 1 roll moveto gsave 0.6 setlinewidth lineto stroke grestore
} bind def
/whf {moveto gsave 0.5 1.2 scale (-) show grestore} bind def

