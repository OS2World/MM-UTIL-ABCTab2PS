    2.5 3 moveto (3) show
  } def
  /italian_4 {
    /Times 20 selectfont 1.5 1.1 scale
    2.5 3 moveto (4) show
  } def
  /italian_5 {
    /Times 20 selectfont 1.5 1.1 scale
    2.5 3 moveto (5) show
  } def
  /italian_6 {
    /Times 20 selectfont 1.5 1.1 scale
    2.5 3 moveto (6) show
  } def
  /italian_7 {
    /Times 20 selectfont 1.5 1 scale
    2.5 3 moveto (7) show
  } def
  /italian_8 {
    /Times 20 selectfont 1.5 1 scale
    2.5 3 moveto (8) show
  } def
  /italian_9 {
    /Times 20 selectfont 1.5 1 scale
    2.5 3 moveto (9) show
  } def
  /italian_10 {
    /Times 20 selectfont 1.5 1.1 scale
    2.5 4 moveto (x) show
  } def
  /italian_11 {
    /Times 20 selectfont 1 0.9 scale
    3 4 moveto (1) show
    9 4 moveto (1) show
  } def
  /italian_12 {
    /Times 20 selectfont 1 0.9 scale
    1.5 4 moveto (1) show
    8.5 4 moveto (2) show
  } def
  /italian_13 {
    /Times 20 selectfont 1 0.9 scale
    1.5 4 moveto (1) show
    8.5 4 moveto (3) show
  } def

  % french bourdons
  % (not used in italiantab)
  /bourdon_4 {
  } def
  /bourdon_5 {
  } def
  /bourdon_6 {
  } def
  /bourdon_7 {
  } def

  % italian bourdons
  /bourdon_8 {
    /Times 20 selectfont 1.5 1.2 scale
    2.5 2 moveto (8) show
  } def
  /bourdon_9 {
    /Times 20 selectfont 1.5 1.2 scale
    2.5 2 moveto (9) show
  } def
