/taba {  % usage: x n taba - ring finger
  -5.5 add -14 mul moveto -2.5 0 rmoveto
  currentpoint 2 copy 1.2 0 360 arc
  currentpoint fill moveto 2.5 0 rmoveto
  currentpoint 2 copy 1.2 0 360 arc
  currentpoint fill moveto 2.5 0 rmoveto
  currentpoint 2 copy 1.2 0 360 arc fill
} bind def

/tabp {  % usage: x n tabp - thumb
  -5.5 add -14 mul moveto
  gsave 1 setlinewidth
  0 2.5 rmoveto 0 -5 rlineto
  stroke grestore
} bind def

/tabpguitar {  % usage: x n tabpguitar - thumb (guitar style)
